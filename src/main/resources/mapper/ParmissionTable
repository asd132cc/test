<mapper namespace="com.ssm.dao.PermissionTableDao">

    <select id="selectPermissionByLimit"  resultType="PermissionTable">
        select * from permissionstable where 1=1 and deletedId =#{deletedId}
        limit ${currPage},${pageSize}
    </select>
</mapper>