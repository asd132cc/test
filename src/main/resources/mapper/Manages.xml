<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.test.mapper.ManageServiceMapper">
    <select id="selectA" resultType="Manages">

        select * from manages where 1=1
    </select>
    <update id="updateA" parameterType="Manages" >
        update manages set solving_doubts=#{Manages.solving_doubts} where id=#{Manages.id}
    </update>




</mapper>